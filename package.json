    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "rollup-plugin-visualizer": "5.6.0",
    "standard-version": "9.5.0",
    "typescript": "4.6.4",
    "unocss": "0.34.1",
    "vite": "4.0.3",
    "vite-plugin-md": "0.13.1",
    "vite-svg-loader": "3.3.0",
    "vue-tsc": "0.34.16",
    "@preconstruct/cli": "2.2.2"
  },
  "preconstruct": {
    "packages": [
      "src/components/*",
      "src/composables",
      "src/directives",
      "src/helpers",
      "src/hooks",
      "src/models",
      "src/templates",
      "src/unocss",
      "src/plugins",
      "src/models/*"
    ],
    "distFilenameStrategy": "unscoped-package-name"
  }
